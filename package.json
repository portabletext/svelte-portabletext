{
  "name": "@portabletext/svelte",
  "version": "2.1.5",
  "homepage": "https://github.com/portabletext/svelte-portabletext#readme",
  "bugs": {
    "url": "https://github.com/portabletext/svelte-portabletext/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portabletext/svelte-portabletext.git"
  },
  "license": "MIT",
  "author": "Sanity <hello@sanity.io>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:site": "svelte-kit build",
    "dev": "svelte-kit dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "eslint --ignore-path .gitignore .",
    "package": "svelte-kit sync && svelte-package",
    "preview": "svelte-kit preview",
    "prepublish": "npm run package && cd dist",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "@portabletext/toolkit": "^2.0.10"
  },
  "devDependencies": {
    "@portabletext/types": "^2.0.8",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.26.0",
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/svelte": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitest/ui": "latest",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^7.0.1",
    "eslint-plugin-svelte": "^2.34.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.12",
    "prettier-plugin-svelte": "^3.2.2",
    "semantic-release": "^23.0.4",
    "svelte": "^4.2.0",
    "svelte-preprocess": "^5.0.4",
    "svelte2tsx": "^0.6.23",
    "typescript": "^5.2.2",
    "vitest": "latest"
  },
  "peerDependencies": {
    "svelte": ">=3.47.0 <5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
